@model IEnumerable<ToDoList.Data.ToDo>
@{
    ViewBag.Title = "Index";
}

<h2>To Do</h2>
<a href="new" class="btn btn-info" role="button">+NewTodo</a>
<table class="table">
    <thead>
        <tr>
            <td>Title</td>
            <td>Priority</td>
            <td>Status</td>
        </tr>
    </thead>
    @foreach (var todo in Model)
    {
       
        <tbody>
            <tr>
                <td>@todo.Title</td>
                <td>@todo.PriorityType.Name</td>
                <td>@todo.IsDone</td>
            </tr>
        </tbody>
    }
</table>